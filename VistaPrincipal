    private void btnRegistrarProveedorActionPerformed(java.awt.event.ActionEvent evt) {                                                      
        // TODO add your handling code here:
        if (txtNombreProveedor.getText().isEmpty()
                || txtTelefonoProveedor.getText().isEmpty()
                || txtCorreoProveedor.getText().isEmpty()
                || txtDireccionProveedor.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null, "DEBE LLENAR TODOS LOS CAMPOS");
        } else {

            String nombre_proveedor = txtNombreProveedor.getText();
            String telefono_proveedor = txtTelefonoProveedor.getText();
            String correo_proveedor = txtCorreoProveedor.getText();
            Pattern pattern = Pattern
                    .compile("^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@"
                            + "[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$");
            Matcher mather = pattern.matcher(correo_proveedor);
            String direccion_proveedor = txtDireccionProveedor.getText();
            if (mather.find()) {
                if (controladorProveedor.SolicitudGuardarProveedor(nombre_proveedor, telefono_proveedor, correo_proveedor, direccion_proveedor)) {
                    JOptionPane.showMessageDialog(this, "El proveedor fue registrado con exito");
                    limpiar();
                    combo();

                } else {
                    JOptionPane.showMessageDialog(this, "error");
                }
            } else {
                JOptionPane.showMessageDialog(null, "El email ingresado es invlido.");

            }
        }
    }                                                     

