    }                                                    

    private void btnRegistrarActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:

        if (txtNombreCategoria.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null, "DEBE LLENAR TODOS LOS CAMPOS");
        } else {

            String nombre_categoria = txtNombreCategoria.getText();

            if (controladorCategoria.SolicitudGuardarCategoria(nombre_categoria)) {
                JOptionPane.showMessageDialog(this, "Categoria registrada con exito");
                limpiar();
                combo();

            } else {
                JOptionPane.showMessageDialog(this, "Error al registrar");

            }
        }
    }                                            

    private void txtNombreCategoriaKeyTyped(java.awt.event.KeyEvent evt) {                                            
        // TODO add your handling code here:
        char validar = evt.getKeyChar();

        if (Character.isDigit(validar)) {
            getToolkit().beep();
            JOptionPane.showMessageDialog(this, " Por favor Ingrese letras");
            evt.consume();

        }
    }                                           

    private void btnEliminarProductoActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        // TODO add your handling code here:
        int codigo = Integer.parseInt(txtId_producto.getText());
        if (controladorProducto.SolicitudEliminar(codigo)) {
            JOptionPane.showMessageDialog(this, "Producto eliminado exitosamente");
            limpiar();

        } else {
            JOptionPane.showMessageDialog(this, "Error al eliminar");
        }
    }                                                   

    private void btnModificarProductoActionPerformed(java.awt.event.ActionEvent evt) {                                                     
        // TODO add your handling code here:

        int id = Integer.parseInt(txtId_producto.getText());
        String nombre_producto = txtNombreProducto.getText();
        String unidad_medida = (String) cbUnidadMedida.getSelectedItem();
        String id_proveedor = (String) cbProveedor.getSelectedItem();
        String id_categoria = (String) cbCategoria.getSelectedItem();

        String idc = (controladorProducto.buscarIdCategoria(id_categoria));
        String idp = (controladorProducto.buscarIdProveedor(id_proveedor));

        if (controladorProducto.SolicitudModificar(id, nombre_producto, unidad_medida, idp, idc)) {
            JOptionPane.showMessageDialog(this, "Producto modificado con exito");
            limpiar();

        } else {
            JOptionPane.showMessageDialog(this, "Error al registrar");

        }
    }                                                    

    private void btnRegistrarProductoActionPerformed(java.awt.event.ActionEvent evt) {                                                     
        // TODO add your handling code here:
        if (txtNombreProducto.getText().isEmpty()
                || cbUnidadMedida.getSelectedIndex() == 0
                || cbCategoria.getSelectedIndex() == 0
                || cbProveedor.getSelectedIndex() == 0) {
            JOptionPane.showMessageDialog(null, "DEBE LLENAR TODOS LOS CAMPOS");
        } else {

            String nombre_producto = txtNombreProducto.getText();
            String unidad_medida = (String) cbUnidadMedida.getSelectedItem();
            String id_proveedor = (String) cbProveedor.getSelectedItem();
            String id_categoria = (String) cbCategoria.getSelectedItem();
            String idc = (controladorProducto.buscarIdCategoria(id_categoria));
            String idp = (controladorProducto.buscarIdProveedor(id_proveedor));

            if (controladorProducto.SolicitudGuardarProducto(nombre_producto, unidad_medida, idp, idc)) {
                JOptionPane.showMessageDialog(this, "producto registrada con exito");
                limpiar();

            } else {
                JOptionPane.showMessageDialog(this, "Error al registrar");

            }
        }
    }                                                    

