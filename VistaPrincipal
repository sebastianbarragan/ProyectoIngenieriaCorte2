    private void txtId_facturaActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
    }                                             

    private void btnRegistrarFacturaActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        // TODO add your handling code here:
        if (cbClienteF.getSelectedIndex() == 0
                || cbVendedorF.getSelectedIndex() == 0
                || fechaFactura.getDate() == null
                || txtDireccionFactura.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null, "DEBE LLENAR TODOS LOS CAMPOS");
        } else {
            String id_cliente = (String) cbClienteF.getSelectedItem();
            String id_vendedor = (String) cbVendedorF.getSelectedItem();
            java.util.Date fecha = fechaFactura.getDate();
            DateFormat f = new SimpleDateFormat("dd-MM-yyyy");
            String fecha2 = f.format(fecha);

            String idc = (controladorFactura.buscarIdCliente(id_cliente));
            String idv = (controladorFactura.buscarIdVendedor(id_vendedor));
            String direccion_factura = txtDireccionFactura.getText();

            if (controladorFactura.SolicitudGuardarFactura(fecha2, direccion_factura, idc, idv)) {
                JOptionPane.showMessageDialog(this, "Factura registrada con exito");
                limpiar();
                combo();

            } else {
                JOptionPane.showMessageDialog(this, "Error al registrar");

            }
        }
    }                                                   

    private void btnModificarFacturaActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        // TODO add your handling code here:
        int id = Integer.parseInt(txtId_factura.getText());
        String id_cliente = (String) cbClienteF.getSelectedItem();
        String id_vendedor = (String) cbVendedorF.getSelectedItem();
        java.util.Date fecha = fechaFactura.getDate();
        DateFormat f = new SimpleDateFormat("dd-MM-yyyy");
        String fecha2 = f.format(fecha);

        String idc = (controladorFactura.buscarIdCliente(id_cliente));
        String idv = (controladorFactura.buscarIdVendedor(id_vendedor));
        String direccion_factura = txtDireccionFactura.getText();
        if (controladorFactura.SolicitudModificar(id, fecha2, direccion_factura, idc, idv)) {
            JOptionPane.showMessageDialog(this, "factura modificada con exito");
            limpiar();

        } else {
            JOptionPane.showMessageDialog(this, "Error al registrar");

        }
    }                                                   

    private void btnEliminarFacturaActionPerformed(java.awt.event.ActionEvent evt) {                                                   
        // TODO add your handling code here:
        int codigo = Integer.parseInt(txtId_factura.getText());
        if (controladorFactura.SolicitudEliminar(codigo)) {
            JOptionPane.showMessageDialog(this, "factua eliminada exitosamente");
            limpiar();

        } else {
            JOptionPane.showMessageDialog(this, "Error al eliminar");
        }
    }                                                  

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
//        try {
            int id_factura = Integer.parseInt((JOptionPane.showInputDialog("Digita el id de la factura")));
            ArrayList<String> cargarTxtFactura = controladorFactura.SolicitudBuscar(id_factura);
            ClsFactura fac = new ClsFactura(Integer.parseInt(cargarTxtFactura.get(0)), cargarTxtFactura.get(4), cargarTxtFactura.get(3), cargarTxtFactura.get(2), cargarTxtFactura.get(1));
            cargarInformacionFactura(fac);
            btnModificarFactura.setEnabled(true);
            btnEliminarFactura.setEnabled(true);
//        } catch (Exception e) {
//            JOptionPane.showMessageDialog(this, "El id a buscar no se encuentra registrado");
//        }
    }                                        
    public void limpiar() {
        txtNombreProducto.setText("");
        txtNombreCategoria.setText("");
        txtNombreProveedor.setText("");
        txtCorreoProveedor.setText("");
        txtDireccionProveedor.setText("");
        cbCategoria.setSelectedIndex(0);
        txtTelefonoProveedor.setText("");
        cbUnidadMedida.setSelectedIndex(0);
        cbProveedor.setSelectedIndex(0);
        cbClienteF.setSelectedIndex(0);

        cbVendedorF.setSelectedIndex(0);
        fechaFactura.setDate(null);
        txtDireccionFactura.setText("");

        txtCedulaCliente.setText("");
        txtNombreCliente.setText("");
        txtApellidoCliente.setText("");
        txtTelefonoCliente.setText("");
        txtCorreoCliente.setText("");
        txtDireccionCliente.setText("");
        txtCedulaVendedor.setText("");
        txtNombreVendedor.setText("");
        txtTelefonoVendedor.setText("");
        txtCorreoVendedor.setText("");
        txtDireccionVendedor.setText("");
    }

    public void combo() {
        cbCategoria.setModel(controladorCategoria.solicitudListar());
        cbProveedor.setModel(controladorProveedor.solicitudListar());
        cbClienteF.setModel(controladorFactura.solicitudListarC());
        cbVendedorF.setModel(controladorFactura.solicitudListarV());

    }

    public void cargarInformacionCategoria(ClsCategoria categoria) {
        txtId_categoria.setText(categoria.getId_categoria() + "");
        txtNombreCategoria.setText(categoria.getNombre_categoria());

    }

    public void cargarInformacionProducto(ClsProducto producto) {
        txtId_producto.setText(producto.getId_producto() + "");
        txtNombreProducto.setText(producto.getNombre_producto());
        cbUnidadMedida.setSelectedItem(producto.getUnidad_medida() + "");
        cbCategoria.setSelectedIndex(Integer.parseInt(producto.getId_categoria()));
        cbProveedor.setSelectedIndex(Integer.parseInt(producto.getId_proveedor()));
    }

    public void cargarInformacionProveedor(ClsProveedor proovedor) {
        txtCodigoProveedor.setText(proovedor.getId_proveedor() + "");
        txtNombreProveedor.setText(proovedor.getNombre_proveedor());
        txtTelefonoProveedor.setText(proovedor.getTelefono_proveedor());
        txtCorreoProveedor.setText(proovedor.getCorreo_proveedor());
        txtDireccionProveedor.setText(proovedor.getDireccion_proveedor());

    }

    public void cargarInformacionCliente(ClsCliente cliente) {
        txtIdCliente.setText(cliente.getId_cliente() + "");
        txtCedulaCliente.setText(cliente.getCedula_cliente() + "");
        txtNombreCliente.setText(cliente.getNombre_cliente());
        txtApellidoCliente.setText(cliente.getApellido_cliente());
        txtTelefonoCliente.setText(cliente.getTelefono_cliente());
        txtCorreoCliente.setText(cliente.getCorreo_cliente());
        txtDireccionCliente.setText(cliente.getDireccion_cliente());

    }

    public void cargarInformacionVendedor(ClsVendedor vendedor) {
        txtIdVendedor.setText(vendedor.getId_vendedor() + "");
        txtCedulaVendedor.setText(vendedor.getCedula_vendedor() + "");
        txtNombreVendedor.setText(vendedor.getNombre_vendedor());
        txtTelefonoVendedor.setText(vendedor.getTelefono_vendedor());
        txtCorreoVendedor.setText(vendedor.getCorreo_vendedor());
        txtDireccionVendedor.setText(vendedor.getDireccion_vendedor());

    }

    public void cargarInformacionFactura(ClsFactura factura) {
        txtId_factura.setText(factura.getId_factura() + "");

       //fechaFactura.setDate(Date.valueOf(factura.getFecha_factura()));
        txtDireccionFactura.setText(factura.getDireccion_envio());
        cbClienteF.setSelectedIndex(Integer.parseInt(factura.getId_cliente()));
        cbVendedorF.setSelectedIndex(Integer.parseInt(factura.getId_vendedor()));

    }

