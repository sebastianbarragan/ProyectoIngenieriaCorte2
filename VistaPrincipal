            getToolkit().beep();
            JOptionPane.showMessageDialog(this, " Por favor Ingrese letras");
            evt.consume();

        }
    }                                          

    private void txtTelefonoVendedorKeyTyped(java.awt.event.KeyEvent evt) {                                             
        // TODO add your handling code here:
        char validar = evt.getKeyChar();

        if (Character.isLetter(validar)) {
            getToolkit().beep();
            JOptionPane.showMessageDialog(this, " Por favor Ingrese numeros");
            evt.consume();

        }
    }                                            

    private void btnBuscarClienteActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        // TODO add your handling code here:
        try {

            int cedula = Integer.parseInt((JOptionPane.showInputDialog("Digita la cedula del cliente")));
            ArrayList<String> cargarTxtCliente = controladorCliente.SolicitudBuscar(cedula);
            ClsCliente cliente = new ClsCliente(Integer.parseInt(cargarTxtCliente.get(0)), Integer.parseInt(cargarTxtCliente.get(1)), cargarTxtCliente.get(2), cargarTxtCliente.get(3), cargarTxtCliente.get(4), cargarTxtCliente.get(5), cargarTxtCliente.get(6));
            cargarInformacionCliente(cliente);
            btnModificarCliente.setEnabled(true);
            btnEliminarCliente.setEnabled(true);
        } catch (Exception e) {
            JOptionPane.showMessageDialog(this, "El id a buscar no se encuentra registrado");
        }

    }                                                

    private void btnBuscarVendedorActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        // TODO add your handling code here:
        try {
            int id_vendedor = Integer.parseInt((JOptionPane.showInputDialog("Digita el id del vendedor")));
            ArrayList<String> cargarTxtVendedor = controladorVendedor.SolicitudBuscar(id_vendedor);
            ClsVendedor vendedor = new ClsVendedor(Integer.parseInt(cargarTxtVendedor.get(0)), Integer.parseInt(cargarTxtVendedor.get(1)), cargarTxtVendedor.get(2), cargarTxtVendedor.get(3), cargarTxtVendedor.get(4), cargarTxtVendedor.get(5));
            cargarInformacionVendedor(vendedor);
            btnModificarVendedor.setEnabled(true);
            btnEliminarVendedor.setEnabled(true);
        } catch (Exception e) {
            JOptionPane.showMessageDialog(this, "El id a buscar no se encuentra registrado");
        }
    }                                                 

