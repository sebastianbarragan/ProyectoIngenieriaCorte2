    private void btBuscarCategoriaActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        try {

            int id_categoria = Integer.parseInt((JOptionPane.showInputDialog("Digita el id de la categoria")));
            ArrayList<String> cargarTxtCategoria = controladorCategoria.SolicitudBuscar(id_categoria);
            ClsCategoria categoria = new ClsCategoria(Integer.parseInt(cargarTxtCategoria.get(0)), cargarTxtCategoria.get(1));
            cargarInformacionCategoria(categoria);
            btnModificategoria.setEnabled(true);
            btnEliminarCategoria.setEnabled(true);

        } catch (Exception e) {
            JOptionPane.showMessageDialog(this, "El id a buscar no se encuentra registrado");
        }

    }                                                 

