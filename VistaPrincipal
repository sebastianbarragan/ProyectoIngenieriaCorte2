    private void btnRegistrarClienteActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        // TODO add your handling code here:
        if (txtCedulaCliente.getText().isEmpty()
                || txtNombreCliente.getText().isEmpty()
                || txtApellidoCliente.getText().isEmpty()
                || txtTelefonoCliente.getText().isEmpty()
                || txtCorreoCliente.getText().isEmpty()
                || txtDireccionCliente.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null, "DEBE LLENAR TODOS LOS CAMPOS");
        } else {
            int cedula_cliente = Integer.parseInt(txtCedulaCliente.getText());
            String nombre_cliente = txtNombreCliente.getText();
            String apellido_cliente = txtApellidoCliente.getText();
            String telefono_cliente = txtTelefonoCliente.getText();
            String correo_cliente = txtCorreoCliente.getText();
            Pattern pattern = Pattern
                    .compile("^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@"
                            + "[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$");
            Matcher mather = pattern.matcher(correo_cliente);
            String direccion_cliente = txtDireccionCliente.getText();
            if (mather.find()) {
                if (controladorCliente.SolicitudGuardarCliente(cedula_cliente, nombre_cliente, apellido_cliente, telefono_cliente, correo_cliente, direccion_cliente)) {
                    JOptionPane.showMessageDialog(this, "El Cliente fue registrado con exito");
                    limpiar();
                    combo();

                } else {
                    JOptionPane.showMessageDialog(this, "error");
                }
            } else {
                JOptionPane.showMessageDialog(null, "El email ingresado es invlido.");

            }
        }

    }                                                   

    private void btnEliminarCategoriaActionPerformed(java.awt.event.ActionEvent evt) {                                                     
