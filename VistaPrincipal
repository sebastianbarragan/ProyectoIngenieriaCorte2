    private void btnVolverPrincipalActionPerformed(java.awt.event.ActionEvent evt) {                                                   
        // TODO add your handling code here:

        Bienvenido fr = new Bienvenido();
        fr.setVisible(true);
        dispose();
    }                                                  

    private void btnEliminarVendedorActionPerformed(java.awt.event.ActionEvent evt) {                                                    
        // TODO add your handling code here:
        int codigo = Integer.parseInt(txtIdVendedor.getText());
        if (controladorVendedor.SolicitudEliminar(codigo)) {
            JOptionPane.showMessageDialog(this, "Vendedor eliminado exitosamente");
            limpiar();

        } else {
            JOptionPane.showMessageDialog(this, "Error al eliminar");
        }
    }                                                   

    private void btnModificarVendedorActionPerformed(java.awt.event.ActionEvent evt) {                                                     
        // TODO add your handling code here:
        int id = Integer.parseInt(txtIdVendedor.getText());
        int cedula_vendedor = Integer.parseInt(txtCedulaVendedor.getText());
        String nombre_vendedor = txtNombreVendedor.getText();
        String telefono_vendedor = txtTelefonoVendedor.getText();
        String correo_vendedor = txtCorreoVendedor.getText();
        String direccion_vendedor = txtDireccionVendedor.getText();

        if (controladorVendedor.SolicitudModificar(id, cedula_vendedor, nombre_vendedor, telefono_vendedor, correo_vendedor, direccion_vendedor)) {
            JOptionPane.showMessageDialog(this, "Modificado exitosamente");
            limpiar();

        } else {
            JOptionPane.showMessageDialog(this, "Error al modificar");
        }
    }                                                    

    private void btnRegistrarVendedorActionPerformed(java.awt.event.ActionEvent evt) {                                                     
        // TODO add your handling code here:
        if (txtCedulaVendedor.getText().isEmpty()
                || txtNombreVendedor.getText().isEmpty()
                || txtTelefonoVendedor.getText().isEmpty()
                || txtCorreoVendedor.getText().isEmpty()
                || txtDireccionVendedor.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null, "DEBE LLENAR TODOS LOS CAMPOS");
        } else {
            int cedula_vendedor = Integer.parseInt(txtCedulaVendedor.getText());
            String nombre_vendedor = txtNombreVendedor.getText();
            String telefono_vendedor = txtTelefonoVendedor.getText();
            String correo_vendedor = txtCorreoVendedor.getText();
            Pattern pattern = Pattern
                    .compile("^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@"
                            + "[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$");
            Matcher mather = pattern.matcher(correo_vendedor);
            String direccion_vendedor = txtDireccionVendedor.getText();
            if (mather.find()) {
                if (controladorVendedor.SolicitudGuardarVendedor(cedula_vendedor, nombre_vendedor, telefono_vendedor, correo_vendedor, direccion_vendedor)) {
                    JOptionPane.showMessageDialog(this, "El Vendedor fue registrado con exito");
                    limpiar();
                    combo();

                } else {
                    JOptionPane.showMessageDialog(this, "error");
                }
            } else {
                JOptionPane.showMessageDialog(null, "El email ingresado es invlido.");

            }
        }
    }                                                    

