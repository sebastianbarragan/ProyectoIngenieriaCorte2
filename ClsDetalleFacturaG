package Modelo;

/**
 *
 * @author PC
 */
public class ClsDetalleFacturaG {
    
   int id_detalle2;
   double subTotal;
   double total;

    public ClsDetalleFacturaG() {
    }

    public ClsDetalleFacturaG(double subTotal, double total) {
        
        this.subTotal = subTotal;
        this.total = total;
    }
   

    public int getId_detalleG() {
        return id_detalle2;
    }

    public void setId_detalleG(int id_detalle2) {
        this.id_detalle2 = id_detalle2;
    }



    public double getSubTotal() {
        return subTotal;
    }

    public void setSubTotal(double subTotal) {
        this.subTotal = subTotal;
    }

    public double getTotal() {
        return total;
    }

    public void setTotal(double total) {
        this.total = total;
    }


  

}
